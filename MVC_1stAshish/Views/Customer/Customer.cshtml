
@model IEnumerable<MVC_1stAshish.Models.Customer>


@{ }

<table class="table table-bordered">
    <tr>
        <th>NAME</th>
        <th>EMAIL</th>
    </tr>
    @foreach (var cust in Model)
    {
        <tr>
            <td>
                @cust.NAME
            </td>
            <td>
                @cust.EMAIL
            </td>


        </tr>
    }
</table>

